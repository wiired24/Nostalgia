{
  "name": "nostalgiajs",
  "version": "1.0.0",
  "description": "NostalgiaJS is a free and open source game launcher",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-win": "electron-packager . --overwrite --platform=win32 --arch=ia32 --icon=assets/icons/Icon_256x256.png --prune=false --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"NostalgiaJS\"",
    "package-linux": "electron-packager . --overwrite --platform=linux --arch=x64 --icon=assets/icons/Icon_256x256.png --prune=false --out=release-builds",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=false --out=release-builds"
  },
  "keywords": [
    "Retro",
    "Gaming",
    "Game",
    "Launcher",
    "Emulation",
    "Emulators"
  ],
  "author": "wiired24",
  "license": "GPL-3.0",
  "devDependencies": {
    "electron": "^12.0.0",
    "electron-packager": "^15.2.0"
  },
  "dependencies": {
    "jquery": "^3.6.0",
    "line-reader": "^0.4.0",
    "recursive-readdir": "^2.2.2",
    "v8-compile-cache": "^2.3.0"
  }
}
